-            pawns = board.BLACK_PAWNS;
-            knights = board.BLACK_KNIGHTS;
-            bishops = board.BLACK_BISHOPS;
-            rooks = board.BLACK_ROOKS;
-            queens = board.BLACK_QUEEN;
-            king = board.BLACK_KING;
